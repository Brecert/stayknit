{
  "name": "sinuous-lifecycle",
  "version": "0.0.0",
  "description": "DOM lifecycles onAttach/onDetach for Sinuous",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/nthm/sinuous-lifecycle"
  },
  "author": "Gen Hames",
  "main": "dist/index.min.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:babel && npm run build:minify",
    "build:babel": "babel --no-babelrc . --ignore 'node_modules' --out-dir dist --extensions \".ts\" --plugins=@babel/plugin-transform-typescript",
    "build:minify": "terser dist/index.js --module --mangle --compress --output dist/index.min.js"
  },
  "peerDependencies": {
    "sinuous": "^0.27.6",
    "sinuous-trace": "file:packages/sinuous-trace"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-typescript": "^7.10.3",
    "terser": "^4.8.0"
  }
}
